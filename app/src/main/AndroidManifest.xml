<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eioms">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Data"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EIOMS">
        <activity
            android:name=".teacher.TeacherActivity"
            android:exported="false"
            android:label="@string/title_activity_teasher"
            android:theme="@style/Theme.EIOMS.NoActionBar" />
        <activity
            android:name=".student.StudentActivity"
            android:exported="false"
            android:label="@string/title_activity_student"
            android:theme="@style/Theme.EIOMS.NoActionBar" />
        <activity
            android:name=".manager.ManagerActivity"
            android:exported="false"
            android:label="@string/title_activity_manager"
            android:theme="@style/Theme.EIOMS.NoActionBar" />
        <activity
            android:name=".LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>